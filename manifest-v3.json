{
	"manifest_version": 3,
	"name": "Isekai and Paper Helper",
	"description": "Isekai and Paper Sounds, Animations, Popups Blocker, and more! ðŸŽ²",
	"version": "0.2",
	"author": "zukini",
	"action": {
		"default_icon": {
			"48": "images/icon48.png"
		},
		"default_title": "Isekai and Paper Helper",
